#include <bits/stdc++.h>
using namespace std;
class graph
{
	vector<int> *v;
	int n;
public:
	graph(int a)
{
		n=a;
		v=new vector<int>[n];

}
};
int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
