#include <bits/stdc++.h>
using namespace std;
class vertex
{
	int time;
	char name;
public:
	vertex(int t,char n)
{
		time=t;
		name=n;
}
	friend class graph;
};
class graph
{
	vector<vector<vertex> >v;

public:
	graph(int a)
{

v.resize(10);
}


};
